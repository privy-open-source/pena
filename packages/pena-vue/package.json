{
  "name": "@privyid/pena-vue",
  "description": "PrivyID's Official Client Integration Library for Vue",
  "version": "1.0.0",
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
