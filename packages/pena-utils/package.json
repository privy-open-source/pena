{
  "name": "@privyid/pena-utils",
  "description": "Shared utils for Pena project",
  "version": "0.3.0",
  "type": "module",
  "main": "./dist/pena-utils.umd.cjs",
  "module": "./dist/pena-utils.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/pena-utils.umd.cjs",
      "import": "./dist/pena-utils.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "preview": "vite preview",
    "prepack": "yarn build",
    "coverage": "vitest run --coverage"
  },
  "browserslist": [
    "extends @privyid/browserslist-config"
  ],
  "devDependencies": {
    "@privyid/browserslist-config": "^0.9.0",
    "@vitest/coverage-c8": "^0.29.0",
    "browserslist-to-esbuild": "^1.2.0",
    "happy-dom": "^8.2.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "0.29.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
