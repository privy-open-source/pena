{
  "name": "@privyid/pena-jquery",
  "description": "PrivyID's Official Client Integration Library for jQuery",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/pena-jquery.umd.cjs",
  "module": "./dist/pena-jquery.js",
  "jsdelivr": "./dist/pena-jquery.iife.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/pena-jquery.umd.cjs",
      "import": "./dist/pena-jquery.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "prepack": "yarn build",
    "dev": "vite",
