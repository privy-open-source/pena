{
  "name": "@privyid/pena-react",
  "description": "PrivyID's Official Client Integration Library for React",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/pena-react.umd.cjs",
  "module": "./dist/pena-react.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/pena-react.umd.cjs",
      "import": "./dist/pena-react.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "prepack": "yarn build",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "browserslist": [
    "extends @privyid/browserslist-config"
  ],
  "peerDependencies": {
    "@privyid/pena": "*",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@privyid/browserslist-config": "0.9.0",
    "@privyid/pena": "workspace:^",
    "@types/react": "18.3.22",
    "@types/react-dom": "18.3.7",
    "@vitejs/plugin-react-swc": "3.3.1",
    "browserslist-to-esbuild": "1.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "4.9.5",
    "vite": "4.0.4",
    "vite-plugin-dts": "2.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
