{
  "name": "@privyid/pena-svelte",
  "description": "PrivyID's Official Client Integration Library for Svelte",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./Pena.svelte": {
      "types": "./dist/Pena.svelte.d.ts",
      "svelte": "./dist/Pena.svelte"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "prepack": "yarn build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest"
  },
  "browserslist": [
    "extends @privyid/browserslist-config"
  ],
  "devDependencies": {
    "@privyid/browserslist-config": "0.9.0",
    "@privyid/pena": "workspace:^",
    "@sveltejs/adapter-auto": "2.1.1",
    "@sveltejs/kit": "1.19.0",
    "@sveltejs/package": "2.0.2",
    "browserslist-to-esbuild": "1.2.0",
    "svelte": "3.55.1",
    "svelte-check": "3.0.3",
    "tslib": "2.5.1",
    "typescript": "4.9.5",
    "vite": "4.0.4",
    "vitest": "0.29.8"
  },
  "peerDependencies": {
    "@privyid/pena": "*",
    "svelte": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
